{
  "name": "@ts-evtx/core",
  "version": "1.0.2",
  "description": "TypeScript library for parsing Windows Event Log (EVTX) files",
  "main": "dist/index.js",
  "type": "module",
  "types": "dist/index.d.ts",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "rm -rf dist && tsc -p tsconfig.build.json && node scripts/postbuild-fix-extensions.mjs",
    "build:all": "npm run build && npm --workspace @ts-evtx/messages run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "example": "ts-node example.ts",
    "prepublishOnly": "npm run build",
    "compare:app": "node debug_scripts/compare-xml-evtx.mjs ./test/fixtures/Application.xml ./test/fixtures/Application.evtx 50"
  },
  "keywords": [
    "evtx",
    "windows",
    "event-log",
    "parser",
    "typescript",
    "binary",
    "forensics"
  ],
  "author": "Nikolai Smetannikov",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NickSmet/ts-evtx.git"
  },
  "homepage": "https://github.com/NickSmet/ts-evtx#readme",
  "bugs": {
    "url": "https://github.com/NickSmet/ts-evtx/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "README.md",
    "package.json"
  ],
  "dependencies": {
    "better-sqlite3": "^11.10.0",
    "crc-32": "^1.2.2",
    "date-fns": "^4.1.0",
    "entities": "^6.0.0",
    "he": "^1.2.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^22.15.30",
    "jest": "^29.7.0",
    "ts-jest": "^29.3.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
